  <h1>Bhavin O'Meter</h1>
  <img src="data:image/jpeg;base64," alt="image1" width="400" height="300" class="center">
  <h3><i>What's Bhavin?</i> Enter your stats to see where you rank on the Bhavin O'Meter</h3>
  <p style="color:grey" font-size:10px;}><i>*Your data cannot and will not be viewed or stored in any way, making this site 100% anonymous</i></p>
  <hr>
    <form id="stats">
      <label for="sat">SAT:</label>
      <input id = "sat"><br>
      <label for="act">ACT (if taken):</label>
      <input id = "act"><br>
      <label for="gpa">Weighted Heritage GPA:</label>
      <input id = "gpa"><br>
      <label for="ap">Number of AP Classes Taken all 4 Years:</label>
      <input id = "ap"><br>
    </form>

    <h2 id="run">Click here to see where you rank on the Bhavin O'Meter!</h2>
    <button id="restart" onClick="window.location.reload();" class="button">Restart</button>
    <br><br><br><br><button id="print" onclick="window.print()" class="button">Print your results!</button>
 
 
<script>
function rank() {
  var sat = document.getElementById("sat").value;  
  var act = document.getElementById("act").value;
  var gpa = document.getElementById("gpa").value;  
  var ap = document.getElementById("ap").value;  
 
    if (sat >= 1570 || act >= 36) {
     sat = 10; //ashwin
    }
    else if (sat >= 1530 || act >= 35){
     sat = 9;
    }
    else if (sat >= 1480 || act >= 34){
     sat = 8; //sarik
    }
    else if (sat >= 1420 || act >= 32){
     sat = 7;
    }
    else if (sat >= 1360 || act >= 30){
     sat = 6; //rich
    }
    else if (sat >= 1300 || act >= 27){
     sat = 5;
    }
    else if (sat >= 1250 || act >= 24){
     sat = 4; //joffe
    }
    else if (sat >= 1175 || act >= 21){
     sat = 3;
    }
    else if (sat >= 1000 || act >= 18){
     sat = 2; //gabby
    }
    else if (sat >= 900 || act >= 15){
     sat = 1;
    }
    else {
     sat = 0;
    }
 
 
    if (gpa >= 5.75) {
     gpa = 10;
    }
    else if (gpa >= 5.6){
     gpa = 9;
    }
    else if (gpa >= 5.5){
     gpa = 8;
    }
    else if (gpa >= 5.3){
     gpa = 7;
    }
    else if (gpa >= 5){
     gpa = 6;
    }
    else if (gpa >= 4.7){
     gpa = 5;
    }
    else if (gpa >= 4.3){
     gpa = 4;
    }
    else if (gpa >= 3.9){
     gpa = 3;
    }
    else if (gpa >= 3.5){
     gpa = 2;
    }
    else if (gpa >= 3){
     gpa = 1;
    }
    else {
     gpa = 0;
    }
 
 
 
   if (ap >= 10) {
     ap = 10;
    }
    else if (ap >= 9){
     ap = 9;
    }
    else if (ap >= 7){
     ap = 8;
    }
    else if (ap >= 6){
     ap = 7;
    }
    else if (ap >= 4){
     ap = 6;
    }
    else if (ap >= 2){
     ap = 5;
    }
    else if (ap >= 1){
     ap = 4;
    }
    else {
     ap = 1;
    }    
    
   
  
  var score = (sat+ap+gpa)/3;
    if (score >= 9.5) {
     score = "Congrats! You are Ashwin Level. You must love math so much!";
    }
    else if (score >= 8.5){
     score = "Congrats, you are Lower Ashwin Level. You must love math so much!";
    }
    else if (score >= 7.5){
     score = "Kudos, you are Sarik Level. Michigan,Michigan,Michigan.";
    }
    else if (score >= 6.5){
     score = "Kudos, you are Lower Sarik Level. Michigan,Michigan,Michigan.";
    }
    else if (score >= 5.5){
     score = "Bro Bro Bro, you are Richard Level. You must love mashed potatoes.";
    }
    else if (score >= 4.5){
     score =  "Bro Bro Bro, you are Richard Level. You must love mashed potatoes.";
    }
    else if (score >= 3.5){
     score =  "*Horse Noise*, you are Olivia Joffe Level.";
    }
    else if (score >= 2.5){
     score = "*Horse Noise*, you are  Lower Olivia Joffe Level.";
    }
    else if (score >= 1.5){
     score = "L Bozo, you are Gabby Level.";
    }
    else if (score >= .5){
     score ="L Bozo, you are Lower Gabby Level.";
    }
    else {
     score = "You did something wrong, try again";
    }
    
  return score;
}
 
document.getElementById("run").onclick = function() {result()};
function result() {
  document.getElementById("run").innerHTML = rank();
}
 
function reset() {
  document.getElementById("stats").reset();
}
 
</script>
 
